pipeline {
    agent any

    stages {
        
        stage('scm checkout') {
            steps {
                echo 'checkout completed'
            }
        
    }
     stage('build') {
            steps {
                echo 'build completed'
            }
        }
         stage('test') {
            steps {
                echo 'test completed'
            }
        }
         stage('deploy') {
            steps {
                echo 'deploy completed'
            }
        }
         
   }
    
     }
